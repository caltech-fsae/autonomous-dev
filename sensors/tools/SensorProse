syntax = "proto2";

message SensorProse {
    /* A world coordinate point with coordinates (x, y, z) for vehicle */
    required double x = 1;
    required double y = 2;
    required double z = 3;

    repeated int32 camera_transformation = 4;
    repeated int32 accel_transformation = 5;
    repeated int32 speed_transformation = 6;
    repeated int32 radar_transformation = 7;
    repeated int32 lidar_transformation = 8;


}
